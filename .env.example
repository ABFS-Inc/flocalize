# Environment Variables Template
# Copy this file to secrets.env and fill in your actual API keys
# DO NOT commit secrets.env to version control

# LLM API Keys
ANTHROPIC_API_KEY=your_anthropic_api_key_here
XAI_API_KEY=your_xai_api_key_here
GOOGLE_API_KEY=your_google_api_key_here

# Google Application Credentials (path to service account JSON file)
GOOGLE_APPLICATION_CREDENTIALS=your_service_account_file.json

# Geoapify API Key (for venue discovery and mapping)
GEOAPIFY_API_KEY=your_geoapify_api_key_here

# Additional API Keys for venue and demographic analysis
SAFEGRAPH_API_KEY=your_safegraph_api_key_here
FOURSQUARE_API_KEY=your_foursquare_api_key_here
INSTAGRAM_ACCESS_TOKEN=your_instagram_token_here
GOOGLE_PLACES_API_KEY=your_google_places_key_here
CENSUS_API_KEY=your_census_api_key_here
